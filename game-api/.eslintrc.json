{
	"extends": "google",
	"env": {
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"max-len": 0,
		"require-jsdoc": 0,
		"indent": 0,
		"no-tabs": 0,
		"prefer-const": 0,
		"no-unused-vars": 0,
		"no-trailing-spaces": 0,
		"semi": 0,
		"comma-dangle": 0,
		"eol-last": 0,
		"quotes": 0
	}
}